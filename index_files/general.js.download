$(window).resize(function () {
    delay(function () {
        is_desktop = viewport().width > 961;
        is_tablet = viewport().width < 961;
        is_mobile = viewport().width < 769;
        is_small_mobile = viewport().width < 500;

        startPageLayout();

    }, 200);
});
